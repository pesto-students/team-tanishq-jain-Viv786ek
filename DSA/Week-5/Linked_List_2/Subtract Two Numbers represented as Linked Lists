function subtractTwoLinkedLists(l1, l2) {
  let head = new ListNode(0);
  let cur = head;let carry = 0;

  while (l1 || l2 || carry) {
    const val1 = l1 ? l1.val : 0;
    const val2 = l2 ? l2.val : 0;

    const diff = val1 - val2 - carry;
    carry = diff < 0 ? 1 : 0;

    const res = (diff + 10) % 10;
    cur.next = new ListNode(res);
    cur = cur.next;

    if (l1) l1 = l1.next;
    if (l2) l2 = l2.next;
  }

  return head.next;
}